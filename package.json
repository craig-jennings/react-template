    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-testing-library": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
