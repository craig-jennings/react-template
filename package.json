{
  "name": "react-template",
  "description": "React Template",
  "version": "0.1.0",
  "author": "Craig Jennings",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:src": "vite build",
    "build:sw": "workbox injectManifest workbox.config.js",
    "ci": "npm-run-all ci:*",
    "ci:chrome": "testcafe chrome:headless ./e2e/**/*.spec.ts --app \"yarn serve\"",
    "ci:firefox": "testcafe firefox:headless ./e2e/**/*.spec.ts --app \"yarn serve\"",
    "prepare": "husky install",
    "serve": "vite preview",
    "start": "vite",
    "test": "npm-run-all test:*",
    "test:e2e": "testcafe chrome ./e2e/**/*.spec.ts --app \"yarn serve\"",
    "test:eslint": "eslint src/ e2e/ --ext js,ts,tsx --quiet",
    "test:jest": "jest",
    "test:typescript": "tsc"
  },
  "dependencies": {
    "history": "5.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.1.1",
    "react-router-dom": "6.0.0-beta.0",
    "styled-components": "5.3.1",
    "workbox-window": "6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "^1.0.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "husky": "^7.0.0",
    "jest": "^27.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "testcafe": "^1.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.5.10",
    "workbox-cli": "^6.0.2"
  }
}
