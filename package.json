    "styled-components": "5.3.5",
    "workbox-window": "6.5.3"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.22.1",
    "@playwright/test": "^1.22.1",
    "@types/facepaint": "^1.2.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
